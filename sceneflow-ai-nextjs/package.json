{
        "name": "sceneflow-ai-nextjs",
        "version": "2.0.1",
        "private": true,
        "main": "electron/main.js",
        "engines": {
                "node": ">=20 <21"
        },
        "scripts": {
                "dev": "next dev --turbo",
                "dev:memory": "NODE_OPTIONS='--max-old-space-size=3072' next dev --turbopack",
                "build": "next build",
                "build:memory": "NODE_OPTIONS='--max-old-space-size=3072' next build",
                "start": "next start",
                "lint": "next lint",
                "electron:dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
                "electron:build": "npm run build && electron-builder --publish never",
                "electron:build:mac": "npm run build && electron-builder --mac dmg zip --publish never",
                "electron:build:win": "npm run build && electron-builder --win nsis --publish never",
                "electron:build:all": "npm run build && electron-builder -mwl --publish never",
                "electron:upload": "node scripts/upload-renderer.js",
                "jobs:process": "ts-node src/scripts/processAiJobs.ts",
        "qa:hybrid": "npm run lint && npx tsc --noEmit && npm run build:memory && ts-node src/scripts/recommendWorkerScale.ts",
                "ci:smoke": "bash scripts/ci/ai-smoke.sh",
                "setup:database": "echo 'Database setup scripts removed during cleanup'",
                "test": "echo 'Test scripts removed during cleanup'",
                "gcp:deploy": "echo 'GCP deployment script removed. Use desktop renderer.'"
        },
        "build": {
                "appId": "ai.sceneflow.desktop",
                "productName": "SceneFlow Desktop Renderer",
                "directories": {
                        "output": "dist/desktop",
                        "buildResources": "electron/resources"
                },
                "files": [
                        "electron/**/*",
                        "desktop/renderer-manifest.json",
                        "package.json",
                        "!**/*.map",
                        "!**/node_modules/.cache/**",
                        "!**/dist/**",
                        "!**/.next/**",
                        "!**/out/**"
                ],
                "extraMetadata": {
                        "main": "electron/main.js"
                },
                "asarUnpack": [
                        "**/node_modules/ffmpeg-static/**",
                        "**/node_modules/ffprobe-static/**"
                ],
                "mac": {
                        "category": "public.app-category.video",
                        "target": [
                                {
                                        "target": "dmg",
                                        "arch": [
                                                "x64",
                                                "arm64"
                                        ]
                                },
                                {
                                        "target": "zip",
                                        "arch": [
                                                "x64",
                                                "arm64"
                                        ]
                                }
                        ],
                        "artifactName": "SceneFlow-Renderer-${version}-${arch}.${ext}"
                },
                "win": {
                        "target": [
                                {
                                        "target": "nsis",
                                        "arch": [
                                                "x64"
                                        ]
                                }
                        ],
                        "artifactName": "SceneFlow-Renderer-${version}-Setup.${ext}"
                },
                "nsis": {
                        "oneClick": true,
                        "perMachine": false,
                        "allowToChangeInstallationDirectory": false,
                        "createDesktopShortcut": true,
                        "shortcutName": "SceneFlow Desktop Renderer",
                        "artifactName": "SceneFlow-Renderer-${version}-Setup.${ext}"
                },
                "publish": null
        },
        "dependencies": {
                "@dnd-kit/core": "^6.3.1",
                "@dnd-kit/sortable": "^10.0.0",
                "@dnd-kit/utilities": "^3.2.2",
                "@google-cloud/firestore": "^7.11.0",
                "@google-cloud/pubsub": "^4.6.0",
                "@google-cloud/storage": "^7.11.3",
                "@google-cloud/text-to-speech": "^6.3.1",
                "@google/generative-ai": "^0.24.1",
                "@neondatabase/serverless": "^1.0.1",
                "@opentelemetry/api": "^1.9.0",
                "@radix-ui/react-dialog": "^1.1.15",
                "@radix-ui/react-dropdown-menu": "^2.1.16",
                "@radix-ui/react-select": "^2.2.6",
                "@radix-ui/react-slot": "^1.2.3",
                "@radix-ui/react-tabs": "^1.1.13",
                "@radix-ui/react-tooltip": "^1.2.8",
                "@react-pdf/renderer": "^4.0.0",
                "@remotion/player": "^4.0.370",
                "@sparticuz/chromium": "^126.0.0",
                "@tiptap/extension-bubble-menu": "^3.3.0",
                "@tiptap/react": "^3.3.0",
                "@tiptap/starter-kit": "^3.3.0",
                "@types/archiver": "^6.0.3",
                "@types/next-pwa": "^5.6.9",
                "@types/uuid": "^10.0.0",
                "@vercel/blob": "^2.0.0",
                "@vercel/postgres": "^0.10.0",
                "archiver": "^7.0.1",
                "bcryptjs": "^2.4.3",
                "class-variance-authority": "^0.7.1",
                "clsx": "^2.1.1",
                "creatomate": "^1.2.1",
                "dotenv": "^17.2.1",
                "fast-xml-parser": "^5.2.5",
                "ffmpeg-static": "^5.2.0",
                "ffprobe-static": "^3.1.0",
                "firebase": "^10.13.1",
                "fluent-ffmpeg": "^2.1.3",
                "fountain-js": "^1.2.4",
                "framer-motion": "^12.23.12",
                "google-auth-library": "^9.11.0",
                "idb": "^8.0.3",
                "inngest": "^3.44.4",
                "jsonwebtoken": "^9.0.2",
                "jszip": "^3.10.1",
                "lodash": "^4.17.21",
                "lucide-react": "^0.539.0",
                "next": "15.4.6",
                "next-auth": "^4.24.7",
                "next-pwa": "^5.6.0",
                "next-themes": "^0.4.6",
                "openai": "^4.52.7",
                "pdfjs-dist": "^5.4.149",
                "pg": "^8.12.0",
                "pg-hstore": "^2.3.4",
                "pino": "^10.1.0",
                "pino-pretty": "^13.1.2",
                "prom-client": "^15.1.3",
                "puppeteer-core": "^23.7.0",
                "react": "19.1.0",
                "react-dom": "19.1.0",
                "react-dropzone": "^14.3.8",
                "react-resizable-panels": "^3.0.5",
                "react-to-print": "^3.2.0",
                "remotion": "^4.0.370",
                "sequelize": "^6.37.7",
                "sharp": "^0.34.4",
                "shotstack-sdk": "^0.2.9",
                "sonner": "^2.0.7",
                "tailwind-merge": "^3.3.1",
                "tailwindcss-animate": "^1.0.7",
                "uuid": "^11.1.0",
                "zod": "^4.0.17",
                "zustand": "^5.0.8"
        },
        "devDependencies": {
                "@eslint/eslintrc": "^3",
                "@types/bcryptjs": "^2.4.6",
                "@types/jsonwebtoken": "^9.0.6",
                "@types/node": "^20",
                "@types/pg": "^8.15.5",
                "@types/react": "^19",
                "@types/react-dom": "^19",
                "@types/sequelize": "^4.28.20",
                "autoprefixer": "^10.4.19",
                "electron": "^31.5.1",
                "electron-builder": "^25.0.5",
                "electron-updater": "^6.6.2",
                "eslint": "^9",
                "eslint-config-next": "15.4.6",
                "postcss": "^8.5.6",
                "tailwindcss": "^3.4.17",
                "ts-node": "^10.9.2",
                "typescript": "^5",
                "vercel": "^46.1.1"
        }
}
