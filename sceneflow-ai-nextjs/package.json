{
  "name": "sceneflow-ai-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup:database": "ts-node src/scripts/setup-database.ts",
    "setup:database:sample": "ts-node src/scripts/setup-database.ts --with-sample-data",
    "test:gateway": "ts-node src/scripts/test-gateway.ts",
    "test:api": "ts-node src/scripts/test-api.ts",
    "test:cue": "ts-node src/scripts/test-cue-api.ts",
    "test:ideas": "ts-node src/scripts/test-idea-generation.ts",
    "test:encryption": "ts-node -e \"const { EncryptionService } = require('./src/services/EncryptionService'); console.log('Encryption configured:', EncryptionService.isEncryptionConfigured());\"",
    "test:database": "ts-node -e \"const { sequelize } = require('./src/config/database'); sequelize.authenticate().then(() => console.log('Database connection successful')).catch(console.error).finally(() => process.exit());\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
