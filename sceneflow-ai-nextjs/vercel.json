{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/vision/generate-script-v2/route.ts": {
      "maxDuration": 600,
      "memory": 3008
    },
    "src/app/api/vision/optimize-script/route.ts": {
      "maxDuration": 600,
      "memory": 3008
    },
    "src/app/api/vision/optimize-script/batch/route.ts": {
      "maxDuration": 120,
      "memory": 2048
    },
    "src/app/api/ideation/film-treatment/route.ts": {
      "maxDuration": 300,
      "memory": 3008
    },
    "src/app/api/treatment/analyze-resonance/route.ts": {
      "maxDuration": 180,
      "memory": 3008
    },
    "src/app/api/v2/blueprint/analyze/route.ts": {
      "maxDuration": 300,
      "memory": 2048
    },
    "src/app/api/v3/blueprint/analyze/route.ts": {
      "maxDuration": 300,
      "memory": 2048
    },
    "src/app/api/character/upload-reference/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/projects/[id]/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/cron/voice-cleanup/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/segments/[segmentId]/upload-video/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "crons": [
    {
      "path": "/api/cron/voice-cleanup",
      "schedule": "0 3 * * *"
    }
  ]
}

